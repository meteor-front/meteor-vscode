/* 样式文件 */
.page {
  display: flex;
  flex-direction: column;
  background-color: #f5f5f5;
  height: 100%;
}
.page > .main {
  flex: 1;
}
.page > .header {
  height: 80rpx;
}
.cell-title {
  margin: 0;
  padding: 32rpx 32rpx 16rpx 32rpx;
  color: rgba(69, 90, 100, 0.6);
  font-weight: normal;
  font-size: 28rpx;
  line-height: 32rpx;
}
